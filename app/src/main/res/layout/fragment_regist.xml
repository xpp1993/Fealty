<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   >
    <LinearLayout
        android:id="@+id/fragment_regist_ll_head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/fragment_regist_iv_callCamera"
            android:layout_width="43dp"
            android:layout_height="35dp"
            android:layout_marginRight="24dp"
            android:src="@mipmap/icon_register_avatar_photo_camera" />

        <ImageView
            android:id="@+id/fragment_regist_iv_head"
            android:layout_width="86dp"
            android:layout_height="86dp"
            android:src="@mipmap/icon_register_avatar_default" />

        <ImageView
            android:id="@+id/fragment_regist_iv_callAlbum"
            android:layout_width="43dp"
            android:layout_height="35dp"
            android:layout_marginLeft="24dp"
            android:src="@mipmap/icon_register_avatar_photo_album" />
    </LinearLayout>


</LinearLayout>
